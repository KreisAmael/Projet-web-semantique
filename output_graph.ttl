@prefix ns1: <http://output.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ns1:Jeff a ns1:Person ;
    rdfs:label "Jeff" ;
    ns1:hasDuration ns1:2_years ;
    ns1:worksFor ns1:Amazon .

ns1:Simon a ns1:Person ;
    rdfs:label "Simon" .

ns1:2_years a ns1:Duration ;
    rdfs:label "2 years" .

ns1:Amazon a ns1:Organization ;
    rdfs:label "Amazon" .

